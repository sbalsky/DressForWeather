    "import schedule\n",
    "import time\n",
    "import os \n",
    "\n",
    "def job():\n",
    "    print('run')\n",
    "    os.system('python Get_Weather.ipynb')\n",
    "    print('done')\n",
    "\n",
    "schedule.every().day.at(\"07:55\").do(job)\n",
    "\n",
    "while True:\n",
    "    schedule.run_pending()\n",
    "    time.sleep(1)"
